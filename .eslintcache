        fontSize: 14,\n    },\n    pos: {\n        marginBottom: 12,\n    },\n}));\n\nconst schema = Yup.object().shape({\n    email: Yup.string().required(\"Campo obrigatório\").min(5, \"Muito curto\"),\n    password: Yup.string().required(\"Campo obrigatório\").min(5, \"Muito curto\"),\n});\n\nconst LoginPage = () => {\n    const history = useHistory();\n    const classes = useStyles();\n\n    const MyTextField = ({ label, ...props }) => {\n        const [field, meta] = useField(props);\n        const errorText = meta.error && meta.touched ? meta.error : \"\";\n\n        return (\n            <TextField\n                fullWidth\n                className={classes.field}\n                variant=\"outlined\"\n                label={label}\n                {...field}\n                helperText={errorText}\n                error={!!errorText}\n            />\n        );\n    }\n\n    const handleSubmit = ( values, { setSubmitting }) => {\n        try {\n            axios.post(\"http://localhost:5000/api/login\", {\n                email: values.email,\n                password: values.password\n            }, {\n                headers: {\n                    'Accept': 'application/json',\n                    'Content-Type': 'application/json'\n                }\n            })\n                .then(res => {\n                    if(res.status === 200) {\n                        localStorage.setItem('token', res.data.token)\n                        //history.push(\"/\");\n                    } else {\n                        const error = new Error(res.error);\n                        throw error;\n                    }\n                })\n                .catch(err => {\n                    console.log(err)\n                });\n        } catch (e) {\n            console.log(e);\n            alert(\"Erro ao logar\");\n        }\n    }\n\n    return (\n        <>\n            <ButtonAppBar />\n            <Container>\n                <Card className={classes.root}>\n                    <CardContent>\n                        <Typography variant=\"h4\" color=\"primary\">Login</Typography>\n                        <Formik initialValues={{ email: '', password: '' }} validationSchema={schema} onSubmit={handleSubmit}>\n                            {({ values, isSubmitting }) => (\n                                <Form>\n                                    <FormControl>\n                                        <MyTextField label=\"Email\" name=\"email\" />\n                                        <MyTextField label=\"Senha\" name=\"password\" />\n                                        <Button type=\"submit\" disabled={isSubmitting} color=\"primary\" variant=\"outlined\">\n                                            Entrar\n                                        </Button>\n                                    </FormControl>\n                                </Form>\n                            )}\n                        </Formik>\n                    </CardContent>\n                </Card>\n            </Container>\n        </>\n    );\n}\n\nexport default LoginPage;","/home/pedror/Projects/repositorio-projetos/src/components/Container.js",[],"/home/pedror/Projects/repositorio-projetos/src/index.js",[],["91","92"],"/home/pedror/Projects/repositorio-projetos/src/pages/About.js",["93"],"/home/pedror/Projects/repositorio-projetos/src/pages/Homepage.js",[],"/home/pedror/Projects/repositorio-projetos/src/pages/NovoAluno.js",[],"/home/pedror/Projects/repositorio-projetos/src/helpers/estados.js",[],"/home/pedror/Projects/repositorio-projetos/src/components/Formulario.js",["94"],"/home/pedror/Projects/repositorio-projetos/src/pages/Projects.js",[],"/home/pedror/Projects/repositorio-projetos/src/components/ProjectForm.js",[],"/home/pedror/Projects/repositorio-projetos/src/pages/Teachers.js",[],"/home/pedror/Projects/repositorio-projetos/src/pages/NewProject.js",[],"/home/pedror/Projects/repositorio-projetos/src/components/TeacherForm.js",["